@model Contract.Dto.PricePredictionDto

@{
    ViewData["Title"] = "Predict price";
}

<h1>Predict houseprice</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="PredictNewPrice">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Date" class="control-label"></label>
                <input asp-for="Date" class="form-control" />
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Bedrooms" class="control-label"></label>
                <input asp-for="Bedrooms" class="form-control" />
                <span asp-validation-for="Bedrooms" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Bathrooms" class="control-label"></label>
                <input asp-for="Bathrooms" class="form-control" />
                <span asp-validation-for="Bathrooms" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_living" class="control-label"></label>
                <input asp-for="Sqft_living" class="form-control" />
                <span asp-validation-for="Sqft_living" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_lot" class="control-label"></label>
                <input asp-for="Sqft_lot" class="form-control" />
                <span asp-validation-for="Sqft_lot" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Floors" class="control-label"></label>
                <input asp-for="Floors" class="form-control" />
                <span asp-validation-for="Floors" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Waterfront" class="control-label"></label>
                <input asp-for="Waterfront" class="form-control" />
                <span asp-validation-for="Waterfront" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="View" class="control-label"></label>
                <input asp-for="View" class="form-control" />
                <span asp-validation-for="View" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Condition" class="control-label"></label>
                <input asp-for="Condition" class="form-control" />
                <span asp-validation-for="Condition" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Grade" class="control-label"></label>
                <input asp-for="Grade" class="form-control" />
                <span asp-validation-for="Grade" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_above" class="control-label"></label>
                <input asp-for="Sqft_above" class="form-control" />
                <span asp-validation-for="Sqft_above" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_basement" class="control-label"></label>
                <input asp-for="Sqft_basement" class="form-control" />
                <span asp-validation-for="Sqft_basement" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Yr_built" class="control-label"></label>
                <input asp-for="Yr_built" class="form-control" />
                <span asp-validation-for="Yr_built" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Yr_renovated" class="control-label"></label>
                <input asp-for="Yr_renovated" class="form-control" />
                <span asp-validation-for="Yr_renovated" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Zipcode" class="control-label"></label>
                <input asp-for="Zipcode" class="form-control" />
                <span asp-validation-for="Zipcode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Lat" class="control-label"></label>
                <input asp-for="Lat" class="form-control" />
                <span asp-validation-for="Lat" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Long1" class="control-label"></label>
                <input asp-for="Long1" class="form-control" />
                <span asp-validation-for="Long1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_living15" class="control-label"></label>
                <input asp-for="Sqft_living15" class="form-control" />
                <span asp-validation-for="Sqft_living15" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sqft_lot15" class="control-label"></label>
                <input asp-for="Sqft_lot15" class="form-control" />
                <span asp-validation-for="Sqft_lot15" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
        <div>
            <a asp-action="GetPredictions" asp-controller="Prediction">See your price predictions</a>
        </div>
    </div>
</div>
<div>
    <a asp-action="Index" asp-controller="Home">Cancel</a>
</div>